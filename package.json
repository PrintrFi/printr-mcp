{
  "name": "@printr/mcp",
  "version": "0.1.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/PrintrFi/printr-mcp"
  },
  "engines": {
    "node": ">=18"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "printr-mcp": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate:api": "bunx openapi-typescript ../printr/backend/golang/spec/omegastar/v0/openapi.yml -o src/api.gen.d.ts",
    "build": "bun build src/index.ts --outdir dist --target node --format esm && tsc --emitDeclarationOnly --outDir dist && chmod +x dist/index.js",
    "dev": "bun --hot src/index.ts",
    "start": "bun src/index.ts",
    "test": "bun test src/lib src/server src/tools src/integration.spec.ts",
    "test:coverage": "bun test --coverage --coverage-reporter=lcov src/lib src/server src/tools src/integration.spec.ts",
    "test:e2e": "bun test src/e2e.spec.ts",
    "test:all": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "bun run typecheck && bun run lint && bun test",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@types/bun": "latest",
    "openapi-typescript": "^7.6.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "hono": "^4.11.10",
    "neverthrow": "^8.2.0",
    "openapi-fetch": "^0.13.0",
    "viem": "^2.46.2",
    "zod": "^4.3.6"
  }
}
